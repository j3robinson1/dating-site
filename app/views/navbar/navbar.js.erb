$('.sidebar').on('click', 'a', function(e) {
    e.preventDefault();
    var thisAtt = $(this).attr('rel');
    console.log(thisAtt);
    if(thisAtt === "connections") {
      $('.navbar').css('width', '100%')
      $(".login-modal").html("<%= escape_javascript(render "connections_links", :formats => [:html]) %>");
    } else {
      $('.navbar').css('width', '100%')
      $(".login-modal").html("<%= escape_javascript(render "profiles_links", :formats => [:html]) %>");
  }
  });